{
  "dashboard": {
    "id": null,
    "title": "AI Bubble Analysis - Longitudinal Dashboard",
    "tags": ["ai", "bubble", "analysis", "time-series", "longitudinal"],
    "timezone": "browser",
    "refresh": "5m",
    "time": {
      "from": "now-30d",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Bubble Risk Evolution",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "average_bubble_risk",
            "legendFormat": "Bubble Risk Score",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0,
            "max": 1,
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 0.4},
                {"color": "red", "value": 0.7}
              ]
            }
          }
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "bottom"
          }
        }
      },
      {
        "id": 2,
        "title": "Market Sentiment Over Time",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "average_sentiment",
            "legendFormat": "Sentiment Score",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": -1,
            "max": 1,
            "thresholds": {
              "steps": [
                {"color": "red", "value": -1},
                {"color": "yellow", "value": -0.3},
                {"color": "green", "value": 0.3},
                {"color": "yellow", "value": 0.7},
                {"color": "red", "value": 1}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Current Risk Level",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "average_bubble_risk",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0,
            "max": 1,
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0, "text": "LOW"},
                {"color": "yellow", "value": 0.4, "text": "MODERATE"},
                {"color": "red", "value": 0.7, "text": "HIGH"}
              ]
            }
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "horizontal"
        }
      },
      {
        "id": 4,
        "title": "Concerning Articles Count",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 8},
        "targets": [
          {
            "expr": "concerning_articles",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 2},
                {"color": "red", "value": 4}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Total Articles Analyzed",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "total_articles",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 6,
        "title": "Analysis Coverage",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 8},
        "targets": [
          {
            "expr": "analyzed_articles / total_articles * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        }
      },
      {
        "id": 7,
        "title": "Bubble Indicators Heatmap",
        "type": "heatmap",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "indicator_value",
            "legendFormat": "{{indicator_name}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0,
            "max": 1
          }
        },
        "options": {
          "calculate": false,
          "cellGap": 2,
          "cellValues": {},
          "color": {
            "mode": "palette-classic"
          },
          "exemplars": {
            "color": "rgba(255,0,255,0.7)"
          },
          "filter": {
            "exclude": false,
            "value": ""
          },
          "legend": {
            "show": true
          },
          "rowsFrame": {
            "layout": "auto"
          },
          "tooltip": {
            "show": true,
            "yHistogram": false
          },
          "yAxis": {
            "axisPlacement": "left",
            "reverse": false,
            "unit": "short"
          }
        }
      },
      {
        "id": 8,
        "title": "Risk Level Distribution",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "count by (market_assessment)",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "pieType": "pie",
          "tooltip": {
            "mode": "single"
          },
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      },
      {
        "id": 9,
        "title": "Indicator Trends",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
        "targets": [
          {
            "expr": "indicator_value",
            "legendFormat": "{{indicator_name}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0,
            "max": 1
          }
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "bottom"
          }
        }
      },
      {
        "id": 10,
        "title": "Bubble Risk vs Sentiment Correlation",
        "type": "scatter",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 28},
        "targets": [
          {
            "expr": "average_bubble_risk",
            "legendFormat": "Bubble Risk",
            "refId": "A"
          },
          {
            "expr": "average_sentiment",
            "legendFormat": "Sentiment",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "single"
          }
        }
      },
      {
        "id": 11,
        "title": "Daily Analysis Summary",
        "type": "table",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 36},
        "targets": [
          {
            "expr": "average_bubble_risk",
            "refId": "A"
          },
          {
            "expr": "average_sentiment",
            "refId": "B"
          },
          {
            "expr": "concerning_articles",
            "refId": "C"
          },
          {
            "expr": "total_articles",
            "refId": "D"
          },
          {
            "expr": "market_assessment",
            "refId": "E"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "options": {
          "showHeader": true,
          "sortBy": [
            {
              "desc": true,
              "displayName": "Time"
            }
          ]
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "time_range",
          "type": "interval",
          "query": "1m,5m,15m,30m,1h,3h,6h,12h,1d,7d,30d",
          "current": {
            "text": "1d",
            "value": "1d"
          }
        },
        {
          "name": "risk_threshold",
          "type": "custom",
          "query": "0.3,0.4,0.5,0.6,0.7,0.8",
          "current": {
            "text": "0.7",
            "value": "0.7"
          }
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "High Risk Events",
          "datasource": "prometheus",
          "enable": true,
          "expr": "average_bubble_risk > 0.7",
          "iconColor": "red",
          "titleFormat": "High Risk Alert",
          "textFormat": "Bubble risk exceeded threshold: {{$value}}"
        },
        {
          "name": "Sentiment Shifts",
          "datasource": "prometheus",
          "enable": true,
          "expr": "abs(delta(average_sentiment[1h])) > 0.3",
          "iconColor": "yellow",
          "titleFormat": "Sentiment Shift",
          "textFormat": "Significant sentiment change detected: {{$value}}"
        }
      ]
    },
    "alerting": {
      "alertRuleTags": {},
      "annotations": {},
      "executionError": "alerting",
      "notifications": []
    }
  }
}
